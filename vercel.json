{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/dist/assets/:path*"
    },
    {
      "source": "/favicon.svg",
      "destination": "/dist/favicon.svg"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html"
    }
  ]
}
